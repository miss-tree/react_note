{
  "name": "reactdemo",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "devDependencies": {
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^8.0.8",
    "koa-static": "^5.0.0"
  },
  "scripts": {
    "start": "cross-env PORT=9527 react-app-rewired start",
    "build": "cross-env GENERATE_SOURCEMAP=false react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-app-rewired eject"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "no-unused-vars": "off",
      "no-array-constructor": "off",
      "eqeqeq": "off",
      "no-redeclare": "off"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all",
    "not ie <= 8"
  ]
}
